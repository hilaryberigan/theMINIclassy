@{
    ViewData["Title"] = "Home Page";
}
<br />

@if (User.IsInRole("Admin"))
{
    @await Html.PartialAsync("_PartialAdminHome");
}
else if(User.IsInRole("Manager"))
{
    @await Html.PartialAsync("_PartialManagerHome");
}
else
{
    @await Html.PartialAsync("_PartialEmployeeHome");

}
